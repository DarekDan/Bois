    <Compile Include="..\Salar.Bois.NetFx\Types\ReflectionHelper.cs" Link="Types\ReflectionHelper.cs" />
    <Compile Include="..\Salar.Bois.NetFx\Types\SharedVariables.cs" Link="Types\SharedVariables.cs" />
